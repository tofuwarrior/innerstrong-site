<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>{{ title or site.brand }}</title>
  <meta name="description" content="{{ description or site.description }}" />
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="/assets/styles.css">
</head>
<body>
  <header class="site-header">
    <div class="container hdr">
<h1 id="logo">
  <a href="/">
    <span class="brand-row">
      {# If you use an image logo, set site.logo in your data and it'll be used #}
      {% if site.logo %}
        <img src="{{ site.logo }}" alt="{{ site.brand }}">
      {% else %}
        <span class="logo-mark">{{ site.brand }}</span>
      {% endif %}

      <span class="brand-word">INNERSTRONG</span>
    </span>

    <span class="brand-subline">{{ site.tagline }}</span>
  </a>
</h1>
      <nav>
        <a href="/">Home</a>
        <a href="/#imagine">Why this works</a>
        <a href="/#benefits">Benefits</a>
        <a href="/#pricing">Pricing</a>
        <a href="/contact">Contact</a>
        <a href="/blog/">Blog</a>
      </nav>
    </div>
  </header>

  <main>
    {{ content | safe }}
  </main>

